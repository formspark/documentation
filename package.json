{
  "name": "formspark-documentation",
  "description": "Formspark documentation",
  "private": true,
  "version": "1.0.0",
  "author": "Bjorn Krols (https://formspark.io)",
  "license": "UNLICENSED",
  "keywords": [
    "documentation",
    "forms",
    "formspark"
  ],
  "authors": {
    "name": "Formspark",
    "email": "support@formspark.io"
  },
  "homepage": "https://github.com/formspark/documentation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formspark/documentation.git"
  },
  "bugs": {
    "url": "https://github.com/formspark/documentation/issues"
  },
  "scripts": {
    "build": "vuepress build docs",
    "deploy": "aws s3 sync docs/.vuepress/dist s3://formspark-production-documentation-website --delete --acl public-read && aws cloudfront create-invalidation --distribution-id E85HAKX4CBCUB --paths \"/*\"",
    "dev": "vuepress dev docs",
    "format": "prettier --write . --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@sentry/browser": "^6.0.4",
    "@sentry/integrations": "^6.0.4",
    "@sentry/tracing": "^6.0.4",
    "lyticus": "^1.0.39",
    "vuepress-plugin-robots": "^1.0.1",
    "vuepress-plugin-sitemap": "^2.3.1"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.8.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "vuepress": "^1.8.0"
  }
}
